<?xml version='1.0' encoding='utf-8'?>
<widget id="hackerspace.silesia.mobile.radiooswiecim" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Radio Oświęcim</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <!-- <author email="" href="http://hs-silesia.pl/"> -->
        <!-- Hackerspace Silesia -->
    <!-- </author> -->

    <author href="http://hs-silesia.pl/">
        Hackerspace Silesia
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <!-- <allow-intent href="tel:*" /> -->
    <!-- <allow-intent href="sms:*" /> -->
    <!-- <allow-intent href="mailto:*" /> -->
    <!-- <allow-intent href="geo:*" /> -->
    <platform name="android">
        <plugin name="Media" value="org.apache.cordova.AudioHandler" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <plugin name="Media" value="CDVSound" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
